    <section class="section-video bg-inverse text-center wp wp-4">
  <video id="video1" class="video-js vjs-default-skin vjs-big-play-centered" controls poster="/grav/user/pages/01.home/_video/video-poster.jpg" data-setup='{}'>
          <source src="http://vjs.zencdn.net/v/oceans.mp4" type='video/mp4'>
          <source src="http://vjs.zencdn.net/v/oceans.webm" type='video/webm'>
      </video>
    <script type="text/javascript">
    function htmlVideo() {
      videojs("video1", {
        controlBar: {
          timeDivider: false,
          fullscreenToggle: false,
          playToggle: false,
          remainingTimeDisplay: false
        },
        "height": "auto",
        "width": "auto"
      }).ready(function() {
        var myPlayer = this;
        var aspectRatio = 5 / 12; // aspect ratio 12:5 (video frame 960x400)
        function resizeVideoJS() {
            var width = document.getElementById(myPlayer.id()).parentElement.offsetWidth;
            myPlayer.width(width).height(width * aspectRatio);
        }
        resizeVideoJS();
        window.onresize = resizeVideoJS;
      });
    }
    </script>
</section>
